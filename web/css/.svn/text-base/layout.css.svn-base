/* CSS Document */

/* ==== Main Layout ==== */
#container, .gray-header, #content, #footer {width:100%;}
#header {background: #ffffff; /* Old browsers */
    /* IE9 SVG, needs conditional override of 'filter' to 'none' */
    background: url(data:image/svg+xml;base64,);
    background: -moz-linear-gradient(left, #ffffff 22%, #478caa 54%, #1d3c5b 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right top, color-stop(22%, #ffffff), color-stop(54%, #478caa), color-stop(100%, #1d3c5b)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(left, #ffffff 22%, #478caa 54%, #1d3c5b 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left, #ffffff 22%, #478caa 54%, #1d3c5b 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(left, #ffffff 22%, #478caa 54%, #1d3c5b 100%); /* IE10+ */
    background: linear-gradient(to right, #ffffff 22%, #478caa 54%, #1d3c5b 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#1d3c5b', GradientType=1 ); /* IE6-8 */
    height: 90px;
    box-shadow: 0 2px 1px #ccc;}

.container {
    padding: 15px 30px;
}
#header .associate-name span.down-arrow {width:11px; height:7px;}
#header .associate-name ul li {width:100px;}
#logo {width:156px;height:46px;}
#footer .green-stripe {height:34px;}
.btn-container {height:55px;}
.btn-container .buttons a {height:30px;}
.lead-search select {height:22px; width:100px;}
.lead-search input[type=submit] {height:23px;}
.lead-search input[type=text] {height:17px; padding:2;width:150px;}

/* ==== Secondary Layout ==== */
.text-field-cell, .text-field-cell input[type=password], .text-field-cell input[type=text] {width:110px;}
.text-field-cell input[type=text], .text-field-cell input[type=password] {height:18px;}
.text-field-cell {width: 167px;}
.error {clear:both;display: block;}
.login-error, .forgot-password {width:100%;}
.btn-login {width:90px;height:28px;}
.welcome-box {width:300px;}
.password-changed {width:286px;height:29px;}
#lead-details .left-box-wrapper {width:100%;}
#lead-details .middle-box {width:500px;}
#lead-details .middle-box-inside {width:90%;min-height:472px;}
.middle-box-inside .headings {width:90px;}
.middle-box-inside .buttons {height:30px;}
#lead-details .right-box {width:25%;min-height:496px;}
.vertical-arrow {width:11px;height:13px;}
.horizontal-arrow {width:13px;height:12px;}
.text-field-style-1 {width:65px;}
.select-style-1 {width:75px;}
.lead-table-edit thead tr th:first-child, .lead-table-edit tbody tr th:first-child {width:30%;}
.lead-table-edit thead tr td:last-child, .lead-table-edit tbody tr td:last-child {width:70%;}
/*Tabs*/
.tabs-container {height:35px;}
/*Tabs Ends*/

