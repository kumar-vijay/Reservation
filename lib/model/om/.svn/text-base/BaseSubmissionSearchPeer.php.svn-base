	public static function retrieveByPK($pk, PropelPDO $con = null)
	{

		if (null !== ($obj = SubmissionSearchPeer::getInstanceFromPool((string) $pk))) {
			return $obj;
		}

		if ($con === null) {
			$con = Propel::getConnection(SubmissionSearchPeer::DATABASE_NAME, Propel::CONNECTION_READ);
		}

		$criteria = new Criteria(SubmissionSearchPeer::DATABASE_NAME);
		$criteria->add(SubmissionSearchPeer::ID, $pk);

		$v = SubmissionSearchPeer::doSelect($criteria, $con);

		return !empty($v) > 0 ? $v[0] : null;
